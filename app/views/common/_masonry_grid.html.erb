<% partial ||= nil %>
<% if !objects.blank? && objects.count > 0 %>
  <ul class="masonry media-grid">
    <% objects.each do |object| %>
      <% if partial %>
        <%= render partial: partial, object: object %>
      <% else %>
        <%= render object %>
      <% end %>
    <% end %>
  </ul>
<% end %>

